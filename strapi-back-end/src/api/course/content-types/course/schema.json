{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "subtitle": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": false
    },
    "overview_image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "current_price": {
      "required": false,
      "type": "integer"
    },
    "discount": {
      "type": "integer",
      "min": 1,
      "max": 100
    },
    "star": {
      "type": "decimal",
      "min": 0,
      "max": 5
    },
    "original_price": {
      "type": "integer",
      "required": true
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "courses"
    },
    "carts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::cart.cart",
      "inversedBy": "courses"
    },
    "favorites": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::favorite.favorite",
      "mappedBy": "courses"
    }
  }
}
